                                            } else {
                                                text = '<div class="row msg_container base_sent"><div class="col-md-10 col-xs-10 " style="width: 80%;"><div class="messages msg_sent"><p>'+json[i].message+'</p><time datetime="'+time+'">Tu</time></div></div><div class="col-md-2 col-xs-2 avatar" style="width: 20%;"><img src="/chat_src/user.png" id="usr_img"></div></div>'
                                            }
                                            $('.msg_container_base').html(cur_text+text);
                                        }
                                    }
                                    var objDiv = $('.msg_container_base');
                                        objDiv.scrollTop($('.msg_container_base')[0].scrollHeight);
                                }
                            }
                        }
                }
            });
